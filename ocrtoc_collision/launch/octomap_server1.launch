<launch>
<node pkg="nodelet" type="nodelet" name="pcl_manager" args="manager" output="screen" />
<!-- Run a VoxelGrid filter to clean NaNs and downsample the data -->
  <node pkg="nodelet" type="nodelet" name="voxel_grid" args="load pcl/VoxelGrid pcl_manager" output="screen">
   <remap from="~input" to="/kinect/depth/points" />
    <rosparam>
      filter_field_name: y
      filter_limit_min: -0.1
      filter_limit_max: 0.3
      filter_limit_negative: False
      leaf_size: 0.05
    </rosparam>
  </node>
   <node pkg="nodelet" type="nodelet" name="voxel_grid2" args="load pcl/VoxelGrid pcl_manager" output="screen">
   <remap from="~input" to="/voxel_grid/output" />
   <remap from="~output" to="/voxel_grid2/output" />
    <rosparam>
      filter_field_name: x
      filter_limit_min: -0.7
      filter_limit_max: +0.7
      filter_limit_negative: False
    </rosparam>
  </node>


</launch>

  

